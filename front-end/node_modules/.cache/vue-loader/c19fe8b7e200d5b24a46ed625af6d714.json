card-meta\n                        title=\"工单预览\"\n                        description=\"点击预览工单信息\">\n                      </a-card-meta>\n                    </router-link>\n                  </a-card>\n                </a-col>\n                <a-col :md=\"24\" :lg=\"6\">\n                  <a-card hoverable>\n                    <img\n                      alt=\"报修单预览\"\n                      src=\"https://res.cloudinary.com/dbmkzs2ez/image/upload/v1562593110/baoxiudan.png\"\n                      slot=\"cover\"\n                    />\n                    <router-link :to=\"{name: 'repairSheet'}\" active-class=\"active\">\n                      <a-card-meta\n                        title=\"报修单预览\"\n                        description=\"点击预览报修单信息\">\n                      </a-card-meta>\n                    </router-link>\n                  </a-card>\n                </a-col>\n                <a-col :md=\"24\" :lg=\"6\">\n                  <a-card hoverable>\n                    <img\n                      alt=\"巡检单预览\"\n                      src=\"https://res.cloudinary.com/dbmkzs2ez/image/upload/v1562593181/xunjiandan.png\"\n                      slot=\"cover\"\n                    />\n                    <router-link :to=\"{name: 'checkSheet'}\" active-class=\"active\">\n                      <a-card-meta\n                        title=\"巡检单预览\"\n                        description=\"点击预览巡检单信息\">\n                      </a-card-meta>\n                    </router-link>\n                  </a-card>\n                </a-col>\n              </a-row>\n            </a-card>\n          </a-card>\n\n          <!--动态\n                    <a-card :loading=\"loading\" title=\"动态\" :bordered=\"false\">\n            <a-list>\n              <a-list-item :key=\"index\" v-for=\"(item, index) in activities\">\n                <a-list-item-meta>\n                  <a-avatar slot=\"avatar\" :src=\"item.user.avatar\"/>\n                  <div slot=\"title\">\n                  